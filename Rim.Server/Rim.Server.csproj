<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Authors>Mehmet Helvacıköylü</Authors>
    <Product>Rim.Server</Product>
    <Description>Advanced WebSocket Server includes basic Http Server</Description>
    <RepositoryUrl>https://github.com/mhelvacikoylu/rimserver.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/mhelvacikoylu/rimserver</PackageProjectUrl>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NeutralLanguage>en</NeutralLanguage>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

</Project>
